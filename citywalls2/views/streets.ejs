<!DOCTYPE html>
<html lang="ru">
<head>
  <meta charset="UTF-8">
  <title>Список улиц</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <header>
    <h1>Список улиц</h1>
    <nav>
      <a href="/">Главная</a>
    </nav>
  </header>
  <main>
    <ul>
      <% streets.forEach(function(street) { %>
        <li>
          <a href="/streets/<%= street._key %>/houses"><%= street.name %></a>
        </li>
      <% }); %>
    </ul>
  </main>
  <footer>
    <p>&copy; 2023 City Data App</p>
  </footer>
</body>
</html>
